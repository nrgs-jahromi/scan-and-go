<script type="text/javascript">
  const signupEmailForm = document.getElementById("form-sign-up-email");
  const newsletterForm = document.getElementById("form-sign-up-newsletter");

  function submitHandler(event) {
    const apiUrl = "http://syntasys.com:8800/subscribe/";
    event.preventDefault();
    const email = event.target.email.value;
    fetch(apiUrl, {
      method: 'POST',
      mode: 'no-cors',
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify({
        email: email
      })
    })

    event.target.email.value = "";
  }
  signupEmailForm.addEventListener("submit", submitHandler);
  newsletterForm.addEventListener("submit", submitHandler);
</script>

